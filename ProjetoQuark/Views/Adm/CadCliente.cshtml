@model ProjetoQuark.Models.ModelCliente
@{
    ViewBag.Title = "CadCliente";

}

<h2>Cadastro de Usuario</h2>

@using (Html.BeginForm("CadCliente", "Adm", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.LabelFor(m => m.nomeCli)
    <br />
    @Html.EditorFor(m => m.nomeCli)
    <br />
    <br />

    @Html.LabelFor(m => m.usuario)
    <br />
    @Html.EditorFor(m => m.usuario)
    <br />
    <br />

    @Html.LabelFor(m => m.emailCli)
    <br />
    @Html.EditorFor(m => m.emailCli)
    <br />
    <br />

    @Html.LabelFor(m => m.senha)
    <br />
    @Html.EditorFor(m => m.senha)
    <br />
    <br />

    <input type="submit" value="Cadastrar" name="btnCad" />
}

@ViewBag.mssge
